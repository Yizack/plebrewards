{
  "name": "plebrewards",
  "version": "1.0.0",
  "description": "Yizack plebrewards",
  "author": "Yizack Rangel",
  "type": "module",
  "homepage": "https://github.com/Yizack/plebrewards",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/plebrewards"
  },
  "bugs": {
    "url": "https://github.com/Yizack/plebrewards/issues"
  },
  "scripts": {
    "build": "pnpm docs:build && nuxt build",
    "dev": "nuxt dev --port=5173 --public",
    "generate": "nuxt generate",
    "preview": "wrangler d1 migrations apply DB --local && wrangler dev",
    "postinstall": "nuxt prepare",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.json",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint --fix --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.css\" --fix --config .config/stylelint.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "(git fetch --unshallow -q || git fetch --all -q) && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test:types": "vue-tsc -b --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/icon": "^2.1.1",
    "@nuxthub/core": "^0.10.4",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/sitemap": "^7.5.0",
    "@types/bootstrap": "^5.2.10",
    "bootstrap": "^5.3.8",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "nuxt-auth-utils": "^0.5.26",
    "nuxt-webhook-validators": "^0.2.6",
    "sass": "^1.97.1",
    "stylelint-config-standard": "^39.0.1",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.2.1",
    "wrangler": "^4.54.0"
  },
  "packageManager": "pnpm@10.26.2"
}
